      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@modpack/core": "workspace:*"
  },
  "devDependencies": {
    "@modpack/typescript-config": "workspace:*"
  }
}