{
  "name": "@modpack/twind",
  "version": "0.2.1-canary.16",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "pub:canary": "pnpm build && pnpm publish --no-git-checks --access public --tag canary",
    "lint": "biome lint"
  },
  "dependencies": {
    "@modpack/utils": "workspace:*",
    "@twind/core": "1.1.3",
    "@twind/preset-autoprefix": "1.0.7",
    "@twind/preset-ext": "1.0.7",
    "@twind/preset-line-clamp": "1.0.7",
    "@twind/preset-tailwind": "1.1.4",
    "@twind/preset-tailwind-forms": "1.1.2"
  },
  "devDependencies": {
    "@modpack/typescript-config": "workspace:*"
  }
}