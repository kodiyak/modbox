{
  "name": "@modpack/core",
  "version": "0.2.1-canary.23",
  "type": "module",
  "private": false,
  "scripts": {
    "test": "vitest",
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "lint": "biome lint",
    "pub:canary": "pnpm build && pnpm publish --no-git-checks --access public --tag canary"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./__internal-tests": {
      "import": "./src/__internal.ts"
    }
  },
  "dependencies": {
    "debug": "4.4.1",
    "@swc/wasm-web": "1.12.9",
    "eventemitter3": "5.0.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@modpack/typescript-config": "workspace:*"
  }
}