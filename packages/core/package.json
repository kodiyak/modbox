{
  "name": "@modpack/core",
  "version": "0.1.2",
  "type": "module",
  "private": false,
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "lint": "biome lint"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "dependencies": {
    "debug": "4.4.1",
    "@swc/wasm-web": "1.12.9",
    "eventemitter3": "5.0.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@modpack/typescript-config": "workspace:*"
  }
}